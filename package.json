{
  "name": "legal-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "powershell -ExecutionPolicy Bypass -File start-dev.ps1",
    "dev:full": "start-dev.bat",
    "start:bat": "start-dev.bat",
    "docker:clean": "docker-compose down && docker stop legal-ai-redis legal-ai-db 2>nul && docker rm legal-ai-redis legal-ai-db 2>nul",
    "docker:up": "docker-compose up -d",
    "prisma:sync": "npx prisma generate && npx prisma db push --accept-data-loss",
    "worker": "cd workers && python document_processor.py",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:migrate:prod": "prisma migrate deploy",
    "clean": "rm -rf .next node_modules/.cache",
    "build:clean": "npm run clean && npm run build",
    "deploy": "bash deploy.sh",
    "docker:build": "docker-compose build --no-cache",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f app"
  },
